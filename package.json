{
  "name": "prelevement-deau-data",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "scripts": {
    "start": "node api.js",
    "init-db": "node scripts/init-db.js",
    "files-validation": "node scripts/files-validation.js",
    "update-db": "node scripts/update-db.js",
    "download-csv": "node scripts/download-csv.js",
    "scalingo-postbuild": "yarn download-csv",
    "lint": "xo",
    "test": "ava"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.689.0",
    "@aws-sdk/lib-storage": "^3.689.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "file-type": "^19.6.0",
    "got": "^14.4.5",
    "http-errors": "^2.0.0",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.12.0",
    "morgan": "^1.10.0",
    "p-limit": "^6.2.0",
    "p-map": "^7.0.3",
    "papaparse": "^5.5.2",
    "proj4": "^2.15.0",
    "wkx": "^0.5.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "ava": "^6.2.0",
    "mongodb-memory-server": "^10.1.3",
    "xo": "^0.58.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "camelcase": "off",
      "object-curly-newline": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-event-target": "off",
      "unicorn/no-array-callback-reference": "off",
      "import/no-unassigned-import": "off",
      "import/order": "off"
    }
  },
  "engines": {
    "node": ">= 22.11 < 23"
  }
}
