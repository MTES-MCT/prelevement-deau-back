{
  "name": "prelevement-deau-data",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "start": "node api.js",
    "init-db": "node scripts/init-db.js",
    "files-validation": "node scripts/files-validation.js",
    "update-db": "node scripts/update-db.js",
    "lint": "xo",
    "test": "ava"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.689.0",
    "@aws-sdk/lib-storage": "^3.689.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "file-type": "^19.6.0",
    "got": "^14.4.3",
    "http-errors": "^2.0.0",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.10.0",
    "morgan": "^1.10.0",
    "p-limit": "^6.1.0",
    "p-map": "^7.0.2",
    "pg": "^8.13.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "ava": "^6.2.0",
    "mongodb-memory-server": "^10.1.2",
    "xo": "^0.58.0"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "object-curly-newline": "off",
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-event-target": "off",
      "unicorn/no-array-callback-reference": "off",
      "import/no-unassigned-import": "off",
      "import/order": "off"
    }
  },
  "engines": {
    "node": ">= 20.9 < 21"
  }
}
